	file = {Full Text:/Users/hopegrismercomputer/Zotero/storage/IACYLVHE/Randolph - 2001 - The shifting landscape of tick-borne zoonoses tick-borne encephalitis and Lyme borreliosis in Europ.pdf:application/pdf},
}

@article{ogden_possible_2021,
