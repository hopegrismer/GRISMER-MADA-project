	number = {6},
	urldate = {2025-05-02},
	journal = {Applied and Environmental Microbiology},
	author = {Ogden, N. H. and Lindsay, L. R. and Hanincová, K. and Barker, I. K. and Bigras-Poulin, M. and Charron, D. F. and Heagy, A. and Francis, C. M. and O'Callaghan, C. J. and Schwartz, I. and Thompson, R. A.},
	month = mar,
